<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="mainPageLoaded" class="page">

 <Page.actionBar>
  <ActionBar title=" Logs" android.iconVisibility="always" class="action-bar">
   <ActionItem tap="add">
    <Label class="menuItem" text="Add " />
   </ActionItem>
   <ActionItem tap="menu">
    <Image src="res://menu" />
   </ActionItem>
  </ActionBar>
 </Page.actionBar>

 <StackLayout style="padding: 10px;">
  <Label id="logHeader" text="Skippers Log" class="center header"/>
  <ScrollView orientation="vertical" height="800">

   <StackLayout id="layout">
    <Label id="" text=" " class="spacer" />
   </StackLayout>

  </ScrollView>

 </StackLayout>

</Page>
